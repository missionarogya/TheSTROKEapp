<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="missionarogya.thestrokeapp" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/heart"
        android:label="ACT FAST - Medical Care and Emergency"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:noHistory="true"
            android:label="ACT FAST - Medical Care and Emergency" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Diagnosis"
            android:noHistory="true"
            android:label="Hospital Information" >
        </activity>
        <activity
            android:name=".EvaluateSymptoms"
            android:noHistory="true"
            android:label="Evaluate the symptoms" >
        </activity>
    </application>

</manifest>
