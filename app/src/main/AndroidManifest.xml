<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="missionarogya.thestrokeapp" >

    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/heart"
        android:label="fastER MED"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="Medical Care and Emergency - ACT FAST"
            android:noHistory="true"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Diagnosis"
            android:label="Diagnosis"
            android:noHistory="true"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".EvaluateSymptoms"
            android:label="Evaluate the symptoms."
            android:noHistory="true" >
        </activity>
        <activity
            android:name=".Icons"
            android:label="Others" >
        </activity>
        <activity
            android:name=".HospitalInformationList"
            android:screenOrientation="portrait"
            android:noHistory="true"
            android:label="STROKE Hospital Information" >
        </activity>
    </application>

</manifest>
